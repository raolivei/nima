# Nima Local Development Environment
# Copy this file to .env.local and update values as needed

# API Port (assigned: 8002 to avoid conflict with swimTO-api and canopy-api)
API_PORT=8002

# Database (assigned: 5434 to avoid conflicts)
DATABASE_URL=postgresql://postgres:postgres@localhost:5434/nima

# Redis (assigned: 6381 to avoid conflicts)
REDIS_URL=redis://localhost:6381/0

# Other settings
SECRET_KEY=your-secret-key-here-change-in-production
DEBUG=True
ENVIRONMENT=development

